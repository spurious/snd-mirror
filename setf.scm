;;; setf/setter written by Maciej Stachowiak

(defmacro setf (place value) 
  (if (pair? place) 
      `((setter ,(car place)) ,@(cdr place) ,value) 
      `(set! place value)))

(define (setter proc) 
  (procedure-property proc 'setter))

(set-procedure-property! setter 'setter 
			 (lambda (proc setter) 
			   (set-procedure-property! proc 'setter setter)))

