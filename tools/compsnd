echo ' '
echo ' '
echo ' --quiet --with-gauche CC=g++ --disable-deprecated CFLAGS="-Wall"'
./configure --quiet --with-gauche CC=g++ --disable-deprecated CFLAGS="-Wall"
make
./snd --version
./snd -noinit --features "'clm 'snd-gauche"
make allclean



echo ' '
echo ' -------------------------------- static gsl -------------------------------- '
make allclean
rm -f snd
