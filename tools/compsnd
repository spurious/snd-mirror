#!/bin/csh -f

fgrep decrip *.html
fgrep accomoda *.html
fgrep pysical *.html
fgrep decrip *.c
fgrep accomoda *.c
fgrep decrip *.scm
fgrep accomoda *.scm
fgrep -e " #(" *.scm
fgrep -e "the the " *.c
fgrep -e "the the " *.html
fgrep udpate *.c
fgrep udpate *.html
fgrep udpate *.scm
fgrep ";;" *.[ch]
va.scm
tools/check-gtk.scm

echo ' -------------------------------- basic configure test -------------------------------- '
rm -f snd
rm -f config.cache
echo ' CFLAGS="-Wall"  '
./configure --quiet CFLAGS="-Wall" 
make allclean
make
echo ' '
echo ' '
make xm
echo ' '
echo ' '
./snd --version
./snd -noinit --features "'clm 'snd-guile 'snd-motif 'sndlib"
echo '(let ((hxm (dlopen (string-append (getcwd) "/xm.so")))) (dlinit hxm "Init_libxm") (display XmARROWS_END) (exit))' > tmp.scm
./snd tmp.scm
rm tmp.scm

sed config.h -e 's/#define HAVE_STRFTIME 1/#define HAVE_STRFTIME 0/' > tmp
mv tmp config.h
make

sed config.h -e 's/#define HAVE_READLINK 1/#define HAVE_READLINK 0/' > tmp
mv tmp config.h
make

sed config.h -e 's/#define HAVE_GETCWD 1/#define HAVE_GETCWD 0/' > tmp
mv tmp config.h
make

sed config.h -e 's/#define HAVE_LSTAT 1/#define HAVE_LSTAT 0/' > tmp
mv tmp config.h
make

sed config.h -e 's/#define HAVE_ACCESS 1/#define HAVE_ACCESS 0/' > tmp
mv tmp config.h
make

sed config.h -e 's/#define HAVE_STRCASECMP 1/#define HAVE_STRCASECMP 0/' > tmp
mv tmp config.h
make

make clmclean
make sndinfo
make audinfo
make sndplay
make sndrecord

# GUILE_CONFIG_path=/home/bil/test/bin 
echo ' -------------------------------- basic configure test/bin -------------------------------- '
make allclean
echo ' '
echo ' '
echo ' CFLAGS="-Wall" GUILE_CONFIG_path=/home/bil/test/bin -Wdeclaration-after-statement -Wmissing-field-initializers'
./configure --quiet CFLAGS="-Wall -Wextra" GUILE_CONFIG_path=/home/bil/test/bin
make
make xm
./snd --version
./snd -noinit --features "'clm 'sndlib 'snd 'snd-guile 'snd-motif"
echo '(let ((hxm (dlopen (string-append (getcwd) "/xm.so")))) (dlinit hxm "Init_libxm") (display XmARROWS_END) (exit))' > tmp.scm
./snd tmp.scm
rm tmp.scm
make allclean

# echo ' CC=g++ CFLAGS="-Wall" GUILE_CONFIG_path=/home/bil/test/bin '
# ./configure --quiet CC=g++ CFLAGS="-Wall" GUILE_CONFIG_path=/home/bil/test/bin
# /usr/include/gmp.h:2073: declaration of C function `std::ostream& 
#   operator<<(std::ostream&, const __mpq_struct*)' conflicts with
# /usr/include/gmp.h:2072: previous declaration `std::ostream& 
#   operator<<(std::ostream&, const __mpz_struct*)' here
# make
# make xm
# snd --version
# make allclean

echo ' '
echo ' '
echo ' --quiet --enable-snd-debug CC=g++ CFLAGS="-Wall"'
./configure --quiet --enable-snd-debug CC=g++ CFLAGS="-Wall"
make
make xm
./snd --version
./snd -noinit --features "'clm 'snd-debug"

echo ' -------------------------------- with-no-gui -------------------------------- '
make allclean
rm -f snd
rm -f config.cache
echo ' '
echo ' '
echo ' CFLAGS="-Wall" --with-no-gui '
./configure --quiet CFLAGS="-Wall" --with-no-gui
make
echo ' '
echo ' '
./snd --version
./snd -noinit --features "'clm 'snd-nogui"

echo ' -------------------------------- with-no-gui/ladspa -------------------------------- '
make allclean
rm -f snd
rm -f config.cache
echo ' '
echo ' '
echo ' CFLAGS="-Wall" --with-no-gui --without-ladspa '
./configure --quiet CFLAGS="-Wall" --with-no-gui --without-ladspa
make
echo ' '
echo ' '
./snd --version
./snd -noinit --features "'clm 'snd-nogui"

echo ' -------------------------------- gtk -------------------------------- '
make allclean
rm -f snd
rm -f config.cache
echo ' '
echo ' '
echo ' CFLAGS="-Wall -Wextra" --with-gtk '
./configure --quiet CFLAGS="-Wall" --with-gtk
make
echo ' '
echo ' '
./snd --version
./snd -noinit --features "'clm 'snd-gtk"
make xg
echo '(let ((hxm (dlopen (string-append (getcwd) "/xg.so")))) (dlinit hxm "Init_libxg") (display GTK_ARROW_UP) (exit))' > tmp.scm
./snd tmp.scm
rm tmp.scm
rm xg.o

echo ' '
echo ' '
echo ' CC=g++ CFLAGS="-Wall" --with-gtk --disable-deprecated '
./configure --quiet CC=g++ CFLAGS="-Wall" --with-gtk --disable-deprecated
make xg
make allclean

echo ' '
echo ' '
echo ' CC=g++ --with-gtk CFLAGS="-Wall" '
./configure --quiet CC=g++ --with-gtk CFLAGS="-Wall"
make
./snd --version
./snd -noinit --features "'clm 'snd-gtk"
make allclean

echo ' '
echo ' '
echo ' CC=g++ --with-gtk CFLAGS="-Wall -DHAVE_GFCDN=0" '
./configure --quiet CC=g++ --with-gtk CFLAGS="-Wall -DHAVE_GFCDN=0"
make
./snd --version
./snd -noinit --features "'clm 'snd-gtk 'snd-guile"
make xg
make allclean

echo ' '
echo ' '
echo ' CC=g++ --with-gtk --without-builtin-gtkrc CFLAGS="-Wall" '
./configure --quiet --with-gtk --without-builtin-gtkrc CFLAGS="-Wall"
make
./snd --version
./snd -noinit --features "'clm 'snd-gtk"

echo ' -------------------------------- no-guile -------------------------------- '
make allclean
rm -f snd
rm -f config.cache
echo ' '
echo ' '
echo ' --without-guile --without-ruby '
./configure --quiet --without-guile --without-ruby
make
echo ' '
echo ' '
./snd --version
./snd -noinit --features ":clm :snd-ruby"
make allclean

echo ' --without-guile --without-ruby --with-gtk '
./configure --quiet --without-guile --without-ruby --with-gtk
make
echo ' '
echo ' '
./snd --version
./snd -noinit --features ":clm :snd-ruby"
rm -f sndinfo
make sndinfo
./sndinfo test.snd
make allclean

echo ' --without-guile --without-ruby --with-gtk --with-gl '
./configure --quiet --without-guile --without-ruby --with-gtk --with-gl
make
echo ' '
echo ' '
./snd --version
./snd -noinit --features "'clm 'snd-gtk 'gl"
make allclean

echo ' --without-guile --without-ruby --with-just-gl '
./configure --quiet --without-guile --without-ruby --with-just-gl
make
echo ' '
echo ' '
./snd --version
./snd -noinit --features "'clm"

echo ' '
echo ' '
echo ' --with-forth'
./configure --with-forth
make
./snd --version
./snd -noinit --features "'clm 'snd-forth 'snd-motif"
make allclean

echo ' '
echo ' '
echo ' --quiet --with-forth --enable-snd-debug --with-static-xm --with-editres --with-gl CFLAGS="-Wall"'
./configure --quiet --with-forth --enable-snd-debug --with-static-xm --with-editres --with-gl CFLAGS="-Wall" --with-midi
make
./snd --version
./snd -noinit --features "'clm 'snd-forth 'xm 'gl"
make allclean

echo ' '
echo ' '
echo ' --quiet --with-forth --with-gtk --with-static-xg --with-gl CFLAGS="-Wall"'
./configure --quiet --with-forth --with-gtk --with-static-xg --with-gl CFLAGS="-Wall"
make
./snd --version
./snd -noinit --features "'clm 'snd-gtk 'xg 'gl"
