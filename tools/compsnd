echo ' '
echo ' '
echo ' --quiet --with-forth --with-no-gui CFLAGS="-Wall"'
./configure --quiet --with-forth --with-no-gui CFLAGS="-Wall"
make
./snd --version
./snd -noinit --features "'clm 'snd-forth 'snd-nogui"
make allclean

echo ' '
echo ' '
echo ' --quiet --with-forth CC=g++ --disable-deprecated CFLAGS="-Wall"'
./configure --quiet --with-forth CC=g++ --disable-deprecated CFLAGS="-Wall"
