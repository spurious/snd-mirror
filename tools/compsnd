./configure --without-guile --without-ruby --with-static-xm --with-gl --quiet
make
./snd --version
./snd -noinit --features "'clm 'xm"
make allclean
echo ' '
echo ' '

echo ' --without-guile --without-ruby --with-gtk --with-static-xg --with-gl --quiet'
./configure --without-guile --without-ruby --with-gtk --with-static-xg --with-gl --quiet
make
./snd --version
./snd -noinit --features "'clm 'xg"
make allclean
echo ' '
echo ' '

echo ' --without-fam --quiet'
./configure --without-fam --quiet
make
./snd --version
./snd -noinit --features "'clm 'snd-guile 'snd-motif 'sndlib"
make allclean
make allclean
echo ' '
echo ' '

echo ' --without-fam --with-gtk --quiet'
./configure --without-fam --with-gtk --quiet
make
./snd --version
./snd -noinit --features "'clm 'snd-gtk"
make allclean

echo ' '
echo ' '
echo ' --enable-threads --quiet'
./configure --enable-threads --quiet
make
./snd --version
./snd -noinit --features "'clm 'snd-guile 'snd-motif"
make allclean

echo ' '
echo ' '
echo ' --enable-threads --quiet CC=g++'
./configure --enable-threads --quiet CC=g++
make
./snd --version
./snd -noinit --features "'clm 'snd-guile"
make allclean

echo ' '
echo ' '
echo ' --enable-threads --with-gtk --with-ruby --quiet'
./configure --enable-threads --with-gtk --with-ruby --quiet
make
./snd --version
./snd -noinit --features ":clm, :snd-gtk, :snd_ruby"
make allclean

echo ' '
echo ' '
echo ' --quiet --with-ruby --with-ruby-prefix=/home/bil/test/tmp-ruby'
./configure --quiet --with-ruby --with-ruby-prefix=/home/bil/test/tmp-ruby
make
./snd --version
./snd -noinit --features ":clm, :snd_ruby"
make allclean

echo ' '
echo ' '
echo ' --with-jack CFLAGS="-I/home/bil/test/include" LDFLAGS="-L/home/bil/test/lib" GUILE_CONFIG_path=/home/bil/test/bin'
./configure --with-jack CFLAGS="-I/home/bil/test/include" LDFLAGS="-L/home/bil/test/lib" GUILE_CONFIG_path=/home/bil/test/bin
make
./snd --version
./snd -noinit --features "'clm 'snd-guile"
make allclean

echo ' '
echo ' '
echo ' --quiet --with-gtk --with-static-xg --with-jack CFLAGS="-I/home/bil/test/include" LDFLAGS="-L/home/bil/test/lib" GUILE_CONFIG_path=/home/bil/test/bin'
./configure --quiet --with-gtk --with-static-xg --with-jack CFLAGS="-I/home/bil/test/include" LDFLAGS="-L/home/bil/test/lib" GUILE_CONFIG_path=/home/bil/test/bin
make
./snd --version
./snd -noinit --features "'clm 'xg 'snd-gtk 'snd-guile"
make allclean

echo ' '
echo ' '
echo ' --quiet --with-snd-as-pd-external --with-no-gui CFLAGS="-I/home/bil/test/include" LDFLAGS="-L/home/bil/test/lib" GUILE_CONFIG_path=/home/bil/test/bin'
./configure --quiet --with-snd-as-pd-external --with-no-gui CFLAGS="-I/home/bil/test/include" LDFLAGS="-L/home/bil/test/lib" GUILE_CONFIG_path=/home/bil/test/bin
make
./snd --version
./snd -noinit --features "'clm 'snd-nogui 'snd-pd-external 'snd-guile"
make allclean

setenv PKG_CONFIG_PATH /home/bil/test/old-gtk/lib/pkgconfig
echo ' '
echo ' '
echo ' --quiet --with-gtk CFLAGS="-Wall -I/home/bil/test/old-gtk"'
./configure --quiet --with-gtk CFLAGS="-Wall -I/home/bil/test/old-gtk"
make
./snd --version
./snd -noinit --features "'clm 'snd-gtk"

setenv PKG_CONFIG_PATH /usr/local/lib/pkgconfig

