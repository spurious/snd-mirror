echo ' '
echo ' '
echo ' --quiet --with-gauche --with-gtk --with-static-xg --with-gl CFLAGS="-Wall"'
./configure --quiet --with-gauche --with-gtk --with-static-xg --with-gl CFLAGS="-Wall"
make
./snd --version
./snd -noinit --features "'clm 'snd-gtk 'xg 'gl"
