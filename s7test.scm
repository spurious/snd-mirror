(define (and-map func . args)
  (call-with-exit
   (lambda (quit)
     (let ((result ()))
       (apply for-each 
	      (lambda arglist
		(let ((val (apply func arglist)))
		  (if (not val)
		      (quit (reverse result))
		      (set! result (cons val result)))))
	    args)
       (reverse result)))))

