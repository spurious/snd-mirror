      (defmacro dotimes (spec . body)
	;; spec = (var end . return)
	(let ((e (gensym))
	      (n (car spec)))
	  `(do ((,e ,(cadr spec))
		(,n 0))
	       ((>= ,n ,e) ,@(cddr spec))
	     ,@body
	     (set! ,n (+ ,n 1)))))

