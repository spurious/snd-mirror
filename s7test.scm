			   (format #t ";~A: add ~A to lists, now not equal?~%" #__line__ a1)
			   (set! lst1 (cdr lst1))
			   (set! lst2 (cdr lst2))))
		     (if (not (equal? v1 v2))
			 (begin
			   (format #t ";~A: add ~A to vectors, now not equal?~%" #__line__ a1)
			   (set! (v1 ctr) #f)
