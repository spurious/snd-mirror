			 '()
			 i)))
		(do ((i (- nd1 1) (- i 1))
		     (j (- nd2 1) (- j 1)))
		    ((or (< i start1)
			 (< j start2)
			 (not (test (key (seq1 i)) (key (seq2 j)))))
		     (if (and (< i start1) (< j start2))
			 '()
			 (+ i 1)))))))
	
	
