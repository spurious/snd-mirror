    (if (not (pair? lst))
	lst
	(let loop ((L lst))
	  (if (pair? (car L))
	      (let ((end (cdr L))
		    (p (car L)))
		(set! (car L) (car p))
		(set! (cdr L) (cdr p))
		(set! (cdr (list-tail L (- (length p) 1))) end)
		(loop L))
	      (if (not (null? (cdr L)))
		  (if (null? (car L))
		      (begin
			(set! (car L) (cadr L))
			(set! (cdr L) (cddr L))
			(loop L))
		      (loop (cdr L)))))
	  (if (equal? lst '(()))
	      '()
	      (let ((len (length lst)))
		(if (null? (car (list-tail lst (- len 1))))
		    (set! (cdr (list-tail lst (- len 2))) '()))
		lst)))))

