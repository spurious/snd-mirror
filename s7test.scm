(let ()
  ;; inspired by Doug Hoyte, "Let Over Lambda"
  (define (mcxr path lst)
    (define (cxr-1 path lst)
      (if (null? path)
	  lst
	  (if (char=? (car path) #\a)
	      (cxr-1 (cdr path) (car lst))
	      (cxr-1 (cdr path) (cdr lst)))))
    (let ((p (string->list (symbol->string path))))
      (if (char=? (car p) #\c)
	  (set! p (cdr p)))
      (let ((p (reverse p)))
	(if (char=? (car p) #\r)
	    (set! p (cdr p)))
	(cxr-1 p lst))))
  
