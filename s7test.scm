		;;   so (sin-m*pi/n 1 60) returns an exact expression for sin(pi/60).
		
		(let ((m (numerator (/ m1 n1)))
		      (n (denominator (/ m1 n1))))
		  
		  (set! m (modulo m (* 2 n)))
		  ;; now it's in lowest terms without extra factors of 2*pi
		  
		  (cond ((zero? m) 0)
			
			((zero? n) (error "divide by zero (sin-m*pi/n n = 0)"))
			
