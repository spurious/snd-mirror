		(define (traverse tree)
		  (if (pair? tree)                                             
		      (cons (traverse (car tree))                             
			    (if (null? (cdr tree)) () (traverse (cdr tree))))
		      (if (memq tree '(and or not)) 
			  tree                 
			  (and (symbol? tree) 
			       (provided? tree)))))
		(if (eval (traverse e))
		    expr
		    (values)))
	      (error "strange #+ chooser: ~S~%" e)))))
  
