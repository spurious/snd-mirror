(define (vector-find-if func vect)
  (let ((len (vector-length vect))
	(result #f))
    (do ((i 0 (1+ i)))
	((or (= i len)
	     result)
	 result)
      (set! result (func (vector-ref vect i))))))


(define* (test-it n cur-min-max cur-min-max-phases :optional tiny)
