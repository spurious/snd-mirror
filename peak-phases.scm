      (let ((try-up #t))
	(let ((happy #t)
	      (cur-phases (vct-copy cur-min-max-phases))
	      (incr -.01))
	  (do ()
	      ((not happy))
	    (vct-set! cur-phases m (+ (vct-ref cur-phases m) incr))
	    (let* ((mod1 (make-noid (radians->hz (/ (* 2 pi) samps)) n cur-phases))
		   (cur-max 0.0))
